#include "carbon-sdm660.dtsi"
/ {
};

&mdss_dsi {
        hw-config = "single_dsi";
};

&mdss_dsi0 {
        qcom,dsi-pref-prim-pan = <&dsi_tianma_fhd_ili7807d_video>;
};

&mdss_dsi1 {
        status = "disabled";
};

&qusb_phy0 {
	qcom,qusb-phy-init-seq = <0xf8 0x80
				0x3f 0x84
				0x83 0x88
				0xc7 0x8c
				0x01 0x90
				0x30 0x08
				0x79 0x0c
				0x21 0x10
				0x14 0x9c
				0x9f 0x1c
				0x00 0x18>;
};

&soc {
	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";
		status = "okay";

		hall_key0 {
			label = "hall_key0";
			gpios = <&tlmm 75 0x01>;
			linux,input-type = <5>;
			linux,code = <0>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};
};

&pm660l_wled {
        qcom,en-cabc;
        qcom,fs-curr-ua = <25000>;
        qcom,led-strings-list = [00 01];
};

&ssc_sensors {
	qcom,firmware-name = "adsp_e4";
};

&qcom_seecom {
	qcom,fde-key-size;
};
